//- index.jade
//- extends ./layout.jade

block row-content
  div.col-lg-6.col-md-6.col-xs-12
    h2 School Matches
    ul
      each match in matches
        //- li= JSON.stringify(match) -- match object here corresponds to the matches pushed in school-finder.js - match.doc provides full access to all school fields
        li= match.score + " score for " + match.school_name + " (" + match.id + ") coords:" + match.doc.location_1.latitude + ":" + match.doc.location_1.longitude
        li
          pre= JSON.stringify(match.traits, null, 4)
    h2 Here is a personality result
    pre= studentPersonality